@page "/fetchusers"
@inject IUserService userService

<h1>Users!</h1>

@if (users == null)
{
    <p><em>Loading...</em></p>
}
else
{
    <table class="table">
        <thead>
            <tr>
                <th>Name</th>
                <th>Surename</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var u in users)
            {
                <tr>
                    <td>@u.Name</td>
                    <td>@u.Surname</td>
                </tr>
            }
        </tbody>
    </table>
}

@code {
    List<UserModel> users = new List<UserModel>();

    protected override async Task OnInitializedAsync()
    {
        users = await userService.GetUsers();
        Console.WriteLine(users);
    }
}
